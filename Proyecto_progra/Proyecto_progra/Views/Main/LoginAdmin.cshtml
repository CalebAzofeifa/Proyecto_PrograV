
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <link href="~/Content/css/styles.css" rel="stylesheet" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script> <!-- Biblioteca jQuery -->
    <script src="~/Scripts/js/logica.js"></script>
</head>
<body>
    <div class="main">
        <form id="login-form">
            <h1>Logo</h1>
            <div class="Login-box">
                <div class="username">
                    <input type="text" id="email" name="CORREO" placeholder="Correo">
                </div>
                <div class="password">
                    <input type="password" id="password" name="CLAVE" placeholder="Contraseña">
                </div>
                <button type="button" id="login_btn" class="login_btn">Iniciar Sesión</button>
            </div>
        </form>
    </div>
</body>
</html>
